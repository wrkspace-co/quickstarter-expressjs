{
  "name": "quickstarter-expressjs",
  "author": "Wrkspace",
  "license": "MIT",
  "version": "1.0.0",
  "description": "A TypeScript starter template for Express.js",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "webpack --config webpack.config.js",
    "lint": "eslint \"src/**/*.ts\"",
    "lint:fix": "eslint \"src/**/*.ts\" --fix",
    "format": "prettier --write \"{src,views}/**/*.{ts,pug,md,json,js}\""
  },
  "dependencies": {
    "express": "^4.18.2",
    "morgan": "^1.10.0",
    "pug": "^3.0.2"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.11",
    "@types/express": "^4.17.16",
    "@types/morgan": "^1.9.4",
    "@types/node": "^20.4.2",
    "@types/nodemon": "^1.19.6",
    "@types/pug": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "autoprefixer": "^10.4.21",
    "css-loader": "^7.1.2",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.3",
    "eslint-webpack-plugin": "^5.0.2",
    "mini-css-extract-plugin": "^2.9.2",
    "nodemon-webpack-plugin": "^4.8.2",
    "postcss": "^8.5.6",
    "postcss-loader": "^8.1.1",
    "prettier": "^3.6.2",
    "tailwindcss": "^4.1.11",
    "ts-loader": "^9.5.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.6",
    "webpack": "^5.100.2",
    "webpack-cli": "^6.0.1",
    "webpack-node-externals": "^3.0.0"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
